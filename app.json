      "image": "redM-shift-schedule-app/assets/calendar2.png",
      "resizeMode": "contain",
      "backgroundColor": "#f5f5f5"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "redM-shift-schedule-app/assets/calendar2.png",
        "backgroundColor": "#f5f5f5"
      },
      "package": "com.redshift.android"
    },
    "notification": {
      "iosDisplayInForeground": true
    },
    "plugins": [
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "66d951a2-7ce2-4270-9e30-f58bd5949cf6"
      }
    }
  }
}
