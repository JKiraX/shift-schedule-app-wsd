{
  "expo": {
    "name": "ShiftX",
    "slug": "redm-shift-scheduler-app",
    "owner": "redm_professional",
    "version": "1.0.0",
    "ios": {
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "icon": "redM-shift-schedule-app/assets/calendar2.png",
